# Plus500 AutoTrader Environment Configuration
# Copy this file to .env and fill in your actual values

# Plus500 Credentials (use demo account for testing)
PLUS500_USERNAME=your_username_here
PLUS500_PASSWORD=your_password_here

# System Configuration
DEBUG=false
HEADLESS=true

# Trading Parameters (optional overrides)
RISK_PERCENTAGE=0.02
STOP_LOSS_PIPS=20
TAKE_PROFIT_PIPS=40
TRADING_START_HOUR=8
TRADING_END_HOUR=18

# Model Configuration (optional overrides)
LSTM_UNITS=50
EPOCHS=100
BATCH_SIZE=32
SEQUENCE_LENGTH=60