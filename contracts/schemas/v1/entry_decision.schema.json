{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$comment": "Timestamp (UTC): 2025-12-22T09:41:32Z",
  "title": "entry_decision (v1)",
  "type": "object",
  "required": [
    "schema_version",
    "kind",
    "run_id",
    "event_time_utc",
    "symbol",
    "decision_time_utc",
    "bar_open_time_utc",
    "bar_close_time_utc",
    "score",
    "threshold",
    "action",
    "planned_entry_time_utc",
    "policy",
    "feature_names",
    "feature_values",
    "model"
  ],
  "properties": {
    "schema_version": { "const": "v1" },
    "kind": { "const": "entry_decision" },
    "run_id": { "type": "string", "minLength": 8 },
    "event_time_utc": { "type": "string", "format": "date-time" },
    "symbol": { "type": "string", "minLength": 3 },
    "decision_time_utc": { "type": "string", "format": "date-time" },
    "bar_open_time_utc": { "type": "string", "format": "date-time" },
    "bar_close_time_utc": { "type": "string", "format": "date-time" },
    "score": { "type": "number" },
    "threshold": { "type": "number" },
    "action": { "type": "string", "enum": ["enter", "hold"] },
    "planned_entry_time_utc": { "type": "string", "format": "date-time" },
    "policy": { "type": "string", "minLength": 3 },
    "feature_names": {
      "type": "array",
      "minItems": 1,
      "items": { "type": "string" }
    },
    "feature_values": {
      "type": "array",
      "minItems": 1,
      "items": { "type": ["number", "null"] }
    },
    "model": {
      "type": "object",
      "required": ["role", "artifact", "created_utc", "features"],
      "properties": {
        "role": { "const": "entry" },
        "artifact": { "type": "string", "minLength": 1 },
        "created_utc": { "type": "string", "minLength": 1 },
        "features": {
          "type": "array",
          "minItems": 1,
          "items": { "type": "string" }
        }
      },
      "additionalProperties": true
    }
  },
  "additionalProperties": true
}
